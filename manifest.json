{
    "name": "EasyColor",
    "description": "A color picker extension",
    "version": "1.0",
    "manifest_version": 3,
    "background":
    {
        "service_worker":"background.js"
    },

"action":{
    "default_popup": "popup.html"
},
   
"permissions":
[
    "activeTab",
    "scripting",
    "storage"
],

"icons":{

    "16": "images/image16.png",
    "32": "images/image32.png",
    "48": "images/image48.png"
}

}